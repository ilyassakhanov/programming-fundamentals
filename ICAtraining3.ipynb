    "def convert_si(val,unit_in,unit_out):\n",
    "    if unit_in=='km':\n",
    "        val=val*1000\n",
    "    if unit_in=='cm':\n",
    "        val=val/100\n",
    "    if unit_out=='km':\n",
    "        val=val*0.001\n",
    "    if unit_out=='cm':\n",
    "        val=val*0.01\n",
    "    return (val)\n",
    "convert_si(30,'cm','m')"
   ]
